{
    "name": "@dojoengine/utils-wasm",
    "version": "1.7.2",
    "description": "dojo: wasm utils ",
    "author": "dojo",
    "license": "MIT",
    "main": "./pkg/utils_wasm.js",
    "type": "module",
    "scripts": {
        "lint:check": "biome lint .",
        "lint": "biome lint . --write",
        "build:wasm": "cd crate && bunx wasm-pack build --out-dir ../pkg --release --features console-error-panic",
        "build": "bun run build:wasm",
        "format:check": "biome format .",
        "format": "biome format . --write"
    },
    "files": [
        "./pkg/utils_wasm_bg.wasm",
        "./pkg/utils_wasm.js",
        "./pkg/utils_wasm_bg.js",
        "./pkg/utils_wasm.d.ts"
    ],
    "module": "./pkg/utils_wasm.js",
    "types": "./pkg/utils_wasm.d.ts",
    "sideEffects": [
        "./pkg/utils_wasm.js",
        "./pkg/snippets/*"
    ],
    "devDependencies": {
        "@biomejs/biome": "^2.1.3",
        "tsup": "^8.3.0",
        "typescript": "^5.6.2",
        "vitest": "^2.1.1",
        "wasm-pack": "^0.12.1"
    }
}
