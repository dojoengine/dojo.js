{
    "name": "@dojoengine/grpc",
    "version": "1.7.4",
    "author": "Dojo Team",
    "license": "MIT",
    "description": "Dojo SDK: Build onchain and provable apps faster",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "lint:check": "biome lint .",
        "lint": "biome lint . --write",
        "build:proto": "./scripts/build-proto.sh",
        "build:deps": "tsup",
        "build": "bun run build:proto && bun run build:deps",
        "format:check": "biome format .",
        "format": "biome format . --write",
        "test": "bun test",
        "bench": "vitest bench --config vitest.config.bench.ts",
        "bench:report": "bun run src/benchmarks/report.ts",
        "bench:full": "bun run bench && bun run bench:report",
        "update:proto": "./scripts/update-proto.sh"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "keywords": [],
    "packageManager": "bun@1.2.19",
    "dependencies": {
        "@grpc/grpc-js": "^1.13.4",
        "@protobuf-ts/grpcweb-transport": "^2.11.1",
        "@protobuf-ts/plugin": "^2.11.1",
        "@protobuf-ts/runtime": "^2.11.1",
        "@protobuf-ts/runtime-rpc": "^2.11.1",
        "effect": "^3.12.6",
        "google-protobuf": "^3.21.4",
        "grpc-web": "^1.5.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.1.3",
        "@dojoengine/internal": "workspace:*",
        "@dojoengine/torii-wasm": "workspace:*",
        "@types/google-protobuf": "^3.15.12",
        "@vitest/coverage-v8": "^1.6.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vite-plugin-wasm": "^3.5.0",
        "vitest": "^1.6.1",
        "websocket": "^1.0.35"
    }
}
