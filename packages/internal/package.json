{
    "name": "@dojoengine/internal",
    "version": "1.6.8-preview.0",
    "private": true,
    "description": "Internal APIs for Dojo Engine packages",
    "author": "Dojo Team",
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build:internal": "tsup",
        "format:check": "biome format .",
        "format": "biome format . --write",
        "lint:check": "biome lint .",
        "lint": "biome lint . --write",
        "test": "vitest run",
        "type:check": "tsc"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "devDependencies": {
        "@biomejs/biome": "^2.1.3",
        "@types/node": "^22.14.1",
        "@vitest/coverage-v8": "^1.6.0",
        "tsup": "^8.4.0",
        "typescript": "^5.7.3",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@dojoengine/core": "workspace:*",
        "@dojoengine/torii-client": "workspace:*",
        "@dojoengine/torii-wasm": "workspace:*",
        "@dojoengine/utils": "workspace:*",
        "starknet": "catalog:",
        "neverthrow": "^8.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dojoengine/sdk.git"
    },
    "bugs": {
        "url": "https://github.com/dojoengine/sdk/issues"
    },
    "homepage": "https://github.com/dojoengine/sdk#readme",
    "keywords": [
        "dojo",
        "internal",
        "engine",
        "typescript"
    ],
    "engines": {
        "node": ">=18.0.0"
    }
}
