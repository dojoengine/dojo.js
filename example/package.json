{
    "name": "example-dojo-showcase",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev:react": "vite --config frameworks/react/vite.config.ts",
        "dev:svelte": "vite --config frameworks/svelte/vite.config.ts",
        "dev:vue": "vite --config frameworks/vue/vite.config.ts",
        "dev:node": "bunx tsx frameworks/node/src/index.ts",
        "dev:worker": "bunx tsx frameworks/worker/src/index.ts",
        "build:react": "vite build --config frameworks/react/vite.config.ts",
        "build:svelte": "vite build --config frameworks/svelte/vite.config.ts",
        "build:vue": "vite build --config frameworks/vue/vite.config.ts",
        "build:node": "tsc -p frameworks/node/tsconfig.json",
        "build:worker": "tsc -p frameworks/worker/tsconfig.json",
        "build": "bun run build:react && bun run build:svelte && bun run build:vue && bun run build:node && bun run build:worker",
        "type:check:react": "tsc -p ./frameworks/react/tsconfig.json ",
        "lint": "biome lint . --write",
        "lint:check": "biome lint .",
        "format": "biome format . --write",
        "format:check": "biome format .",
        "check": "tsc --noEmit -p tsconfig.json && bun run build:node && bun run build:worker"
    },
    "dependencies": {
        "@dojoengine/core": "workspace:*",
        "@dojoengine/create-burner": "workspace:*",
        "@dojoengine/grpc": "workspace:*",
        "@dojoengine/react": "workspace:*",
        "@dojoengine/recs": "2.0.13",
        "@dojoengine/sdk": "workspace:*",
        "@dojoengine/state": "workspace:*",
        "@dojoengine/torii-wasm": "catalog:",
        "@dojoengine/torii-client": "catalog:",
        "@dojoengine/utils": "workspace:*",
        "@dojoengine/internal": "workspace:*",
        "@effect-atom/atom-react": "^0.4.0",
        "@latticexyz/utils": "^2.2.8",
        "@tanstack/react-router": "^1.87.3",
        "effect": "^3.19.4",
        "react": "catalog:",
        "react-dom": "catalog:",
        "starknet": "catalog:",
        "svelte": "^4.2.18",
        "vue": "^3.4.38"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.1.3",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@tsconfig/svelte": "^5.0.4",
        "@types/node": "^20.16.6",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitejs/plugin-react": "^4.3.1",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/compiler-sfc": "^3.4.38",
        "svelte-check": "^3.8.5",
        "tslib": "^2.6.3",
        "tsx": "^4.16.2",
        "typescript": "^5.6.2",
        "vite": "^5.4.1",
        "vite-plugin-top-level-await": "^1.4.4",
        "vite-plugin-wasm": "^3.3.0"
    }
}
